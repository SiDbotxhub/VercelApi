{
  "version": 2,
  "builds": [
    {
      "src": "api/song.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "includeFiles": "lib/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/song/(.*)",
      "dest": "api/song.py",
      "methods": ["GET"]
    },
    {
      "src": "/health",
      "dest": "api/song.py",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "api/song.py",
      "methods": ["GET"]
    }
  ]
